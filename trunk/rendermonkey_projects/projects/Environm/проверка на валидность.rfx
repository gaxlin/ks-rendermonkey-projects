float vRot;
float4 vViewPosition;
float vTrnsl;
float4x4 f4x4MyMatrix;
 
float4x4 matViewProjection;
 
struct VS_OUTPUT 
{
   float4 Pos : POSITION;
   float2 Tex : TEXCOORD0;
};

VS_OUTPUT vs_main( float4 inPos: POSITION, float2 inTex: TEXCOORD )
{
   VS_OUTPUT Out;
  
// matWVP = mul ( f4x4MyMatrix,   matViewProjection  );

float sinAng, cosAng;

sincos(vRot,   sinAng,   cosAng   ); 

float4x4 mTrnsf = 0.0;
mTrnsf[0][0] = 1.0;
mTrnsf[1][1] = 1.0;
mTrnsf[2][2] = 1.0;
mTrnsf[3][3] = 1.0;

float4x4 mRotateY = 0.0;
mRotateY[0][0] = 1.0; 
mRotateY[1][1] = 1.0;
mRotateY[2][2] = 1.0;
mRotateY[3][3] = 1.0;

 mRotateY[0][0] = cosAng;  
 mRotateY[2][0] = -sinAng;  
 mRotateY[0][2] = sinAng;
 mRotateY[2][2] = cosAng;



 mTrnsf[0][0] = cosAng;  
 mTrnsf[2][0] = -sinAng;  
 mTrnsf[0][2] = sinAng;
 mTrnsf[2][2] = cosAng; 


mTrnsf[0][3] =+   vViewPosition[0];
mTrnsf[1][3] =+   vViewPosition[1];
mTrnsf[2][3] =+   vViewPosition[2]; 
  
 

  
float4x4 mRes = mul (  matViewProjection, mTrnsf);

   Out.Pos = mul( mRes, inPos );
   
   Out.Tex = inTex;

   return Out;
}



]]></RmShaderCode>
