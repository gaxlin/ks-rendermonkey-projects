      <RmShaderConstant NAME="Ka" API="" REGISTER="4"/>
      <RmShaderConstant NAME="Kd" API="" REGISTER="7"/>
      <RmShaderConstant NAME="bumpiness" API="" REGISTER="3"/>
      <RmShaderConstant NAME="diffuse" API="" REGISTER="2"/>
      <RmShaderConstant NAME="specular" API="" REGISTER="1"/>
