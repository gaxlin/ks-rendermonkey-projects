     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="vs_main"><RmShaderCode TYPE="TEXT"><![CDATA[
float time_0_X;
float4x4 view_proj_matrix;
 

struct VS_OUTPUT 
{
   float4 Pos : POSITION;
   float2 Tex : TEXCOORD0;
};

VS_OUTPUT vs_main( float4 inPos: POSITION, float2 inTex: TEXCOORD )
{
   VS_OUTPUT Out;


     float4 modPos = inPos;
     modPos[0] = modPos[0] * 1.5;
 modPos[2] =+ (   ( sin(  time_0_X * 5 + modPos[0]* 2.0  ))* 0.3   );

         modPos[3] = inPos[3];


   Out.Pos = mul( view_proj_matrix, modPos );
   
   Out.Tex = inTex;

   return Out;
}



