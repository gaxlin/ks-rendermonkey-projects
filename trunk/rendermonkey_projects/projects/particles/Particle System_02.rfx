     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="main" COMPILER_FLAGS="32"><RmShaderCode TYPE="TEXT"><![CDATA[
sampler Flame: register(s1);
sampler Volume: register(s0);
float4 main(float3 pos: TEXCOORD0, float2 texCoord: TEXCOORD1, float color: TEXCOORD2) : COLOR {
   float4 col = tex3D(Volume, pos);

   return (0.1 + 0.9 * col) * (1 - pow(dot(texCoord, texCoord), 0.3)) * tex2D(Flame, float2(color,0.5f));
}

]]></RmShaderCode>
      <RmSampler NAME="Flame" API="" REGISTER="1"/>
