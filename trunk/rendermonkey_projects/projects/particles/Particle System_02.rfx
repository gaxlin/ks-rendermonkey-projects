     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="main" COMPILER_FLAGS="32"><RmShaderCode TYPE="TEXT"><![CDATA[
float snakeSpeed;
 
float4x4 view_proj_matrix: register(c0);
float4x4 g_matrView: register(c4);
float time_0_X: register(c8);
struct VS_OUTPUT {
   float4 Pos: POSITION;
   float2 texCoord: TEXCOORD0;
   float color: TEXCOORD1;
};

VS_OUTPUT main(float4 Pos: POSITION){
   VS_OUTPUT Out;

   float3 pos;
   // Billboard the quads.
   // The view matrix gives us our right and up vectors.
   pos = 15 * Pos.z * (Pos.x * g_matrView[0] + Pos.y * g_matrView[1]);

   // Move the quads around along some odd path
   float t = (time_0_X * snakeSpeed) + 4 * Pos.z;
   pos.x += 50 * cos(1.24 * t) * sin(3.12 * t);
   pos.y += 50 * sin(2.97 * t) * cos(0.81 * t);
   pos.z += 50 * cos(t) * sin(t * 1.231);

   Out.Pos = mul(view_proj_matrix, float4(pos, 1));
   Out.texCoord = Pos.xy;
   Out.color = Pos.z;

   return Out;
}]]></RmShaderCode>
      <RmShaderConstant NAME="g_matrView" API="" REGISTER="4"/>
      <RmShaderConstant NAME="snakeSpeed" API="" REGISTER="9"/>
      <RmShaderConstant NAME="time_0_X" API="" REGISTER="8"/>
      <RmShaderConstant NAME="view_proj_matrix" API="" REGISTER="0"/>
     </RmHLSLShader>
     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="main" COMPILER_FLAGS="32"><RmShaderCode TYPE="TEXT"><![CDATA[
float particleExp: register(c0);
sampler Palette: register(s0);
float4 main(float2 texCoord: TEXCOORD0, float color: TEXCOORD1) : COLOR {
   return (1 - pow(dot(texCoord, texCoord), particleExp)) * tex2D(Palette, float2( color, 0.5f ));
}

]]></RmShaderCode>
