     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[
//float4 coronaDir: register(c1);
float4 view_direction: register(c2);
float coronaExp: register(c3);
float strength: register(c4);
float4 lightColor: register(c0);
sampler Corona: register(s0);

float4 main(float2 texCoord: TEXCOORD0) : COLOR {
   float4 corona = tex2D(Corona, texCoord);

   // Glow depending on cosine (dotprod) with "corona directon"
   float glow = saturate(/*dot(view_direction.xyz, coronaDir.xyz)*/ 5.5);

   return strength * pow(glow, coronaExp) * corona * lightColor;
}







]]></RmShaderCode>
      <RmSampler NAME="Corona" API="" REGISTER="0"/>
      <RmShaderConstant NAME="lightColor" API="" REGISTER="0"/>
      <RmShaderConstant NAME="strength" API="" REGISTER="4"/>
