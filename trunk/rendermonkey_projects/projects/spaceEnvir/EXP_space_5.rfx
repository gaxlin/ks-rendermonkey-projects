     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_1_1" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 matViewProj: register(c0);
float4x4 matView: register(c4);
float4 posit: register(c8);
float coronaSize: register(c9);

struct VS_OUTPUT {
   float4 Pos: POSITION;
   float2 texCoord: TEXCOORD0;
};

VS_OUTPUT main(float4 Pos: POSITION)
{
   VS_OUTPUT Out;

   // Grab X and Y eye-space vectors in world-space ...
   float3 dirX = matView[0] * 1 ;   // * 4 ;
   float3 dirY = matView[1] * 1 ;   // * 0.5 ;

   // ... which are used for billboarding
   float3 pos = posit + coronaSize * (Pos.x * dirX * 1 + Pos.y * dirY * 1);

   Out.Pos = mul(matViewProj, float4(pos, 1));
   
   Out.texCoord = 0.5 * Pos.xy + 0.5; //Out.texCoord = 0.5 * Pos.xy + 0.5;

   return Out;
}




]]></RmShaderCode>
