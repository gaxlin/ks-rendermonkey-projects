     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[

float3 planetPosition; 
float3 starPosition;

float planetVerticScale;
float planetRadius;

float4x4 matView;
float4x4 matViewProjection;


struct VS_OUTPUT
{
   float4 Pos    : POSITION;
   float3 Norm   : TEXCOORD0;
   float3 View   : TEXCOORD1;
   float3 Light  : TEXCOORD2;
   float2 Tex    : TEXCOORD4;
};

float4x4 MatrixScale (float x, float y, float z)
{
float4x4 mRes;
 mRes[0][0]  = x;   mRes[0][1]   = 0;    mRes[0][2]  = 0;   mRes[0][3]   = 0;
 
 mRes[1][0]  = 0;   mRes[1][1]   = y;    mRes[1][2]  = 0;   mRes[1][3]   = 0;
 
 mRes[2][0]  = 0;   mRes[2][1]   = 0;    mRes[2][2]  = z;   mRes[2][3]   = 0; 
 
 mRes[3][0]  = 0;   mRes[3][1]   = 0;    mRes[3][2]  = 0;   mRes[3][3]   = 1;
return mRes;
};//======================================================================================

VS_OUTPUT main( 
   float4 inPos  : POSITION,
   float3 inNorm : NORMAL,
   float2 inTex  : TEXCOORD0 )
{
   VS_OUTPUT Out = (VS_OUTPUT) 0; 

float4 modPos = inPos * planetRadius;
       modPos[3] = inPos[3]; 

float4x4 matSc = MatrixScale(1, planetVerticScale, 1);
float4x4 matTemp = mul (matViewProjection, matSc);

   // Output transformed position:
   Out.Pos = mul( matTemp, modPos ); 
   
   // Output light vector:
 float4 lightDirMod ;
    lightDirMod[0] = starPosition[0] - planetPosition[0];
    lightDirMod[1] = starPosition[1] - planetPosition[1];
    lightDirMod[2] = starPosition[2] - planetPosition[2];
    lightDirMod[3] = 1; 
// float3 planetPosition; 
//float3 starPosition;  
   
   Out.Light =   normalize(lightDirMod); //normalize(lightDir);

   // Compute position in view space:
   float3 Pview = mul( matView, modPos ); 

   // Transform the input normal to view space:
   Out.Norm = normalize(   inNorm   );  //Out.Norm = normalize( mul( matView, inNorm ) );  
 
   // Compute the view direction in view space:
   Out.View =   normalize( Pview );

   // Propagate texture coordinate for the object:
   Out.Tex = inTex;

   return Out;
}




]]></RmShaderCode>
