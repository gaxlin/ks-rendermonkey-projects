     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="vs_main"><RmShaderCode TYPE="TEXT"><![CDATA[
float planetRadius;
float ringRadius;
float4x4 matViewProjection;

float4x4 MatrixRotateX (float ang)
{
float4x4 mRes  ;

   float sn;
   float cs;
sincos(ang, sn, cs);

 mRes[0][0]  = 1;   mRes[0][1]   = 0;    mRes[0][2]  = 0;   mRes[0][3]   = 0;
 
 mRes[1][0]  = 0;   mRes[1][1]   = cs;    mRes[1][2]  = sn;   mRes[1][3]   = 0;
 
 mRes[2][0]  = 0;   mRes[2][1]   = -sn;    mRes[2][2]  = cs;   mRes[2][3]   = 0; 
 
 mRes[3][0]  = 0;   mRes[3][1]   = 0;    mRes[3][2]  = 0;   mRes[3][3]   = 1;

return mRes;
};//=====================================================================================






struct VS_OUTPUT 
{
   float4 Pos : POSITION;
   float2 Tex : TEXCOORD0;
};

VS_OUTPUT vs_main( float4 inPos: POSITION, float2 inTex: TEXCOORD )
{
   VS_OUTPUT Out;
   
   float4 modPos;
   modPos[0] = inPos[0] * ringRadius * planetRadius;
   modPos[1] = inPos[1] * ringRadius * planetRadius;   
   modPos[2] = inPos[2] * ringRadius * planetRadius;   
   modPos[3] = inPos[3] ;  


float pi = 3.1415926535897932384626433832795;
float4x4 matRot = MatrixRotateX(pi);
float4x4 matTemp = mul (matViewProjection, matRot);


   Out.Pos = mul( matTemp, modPos ); // modPos
   
   Out.Tex = inTex;

   return Out;
}



]]></RmShaderCode>
