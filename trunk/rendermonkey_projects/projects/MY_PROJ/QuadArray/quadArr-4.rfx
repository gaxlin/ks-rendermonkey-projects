     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="vs_main"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 matView;
float4x4 matViewProj;
float f1MyFloat_2;
float f1MyFloat;
float time;
 

struct VS_OUTPUT 
{
   float4 Pos : POSITION;
   float2 Tex : TEXCOORD0;
};

static const int VECCOUNT = 3;

float4 vectors[VECCOUNT] =
{
   {   3,  3,  3,  1 },
   {   0,  0,  0,  1 },
   {  -1, -1, -1,  1 }
};

VS_OUTPUT vs_main( float4 inPos: POSITION, float2 inTex: TEXCOORD )
{
   VS_OUTPUT Out;
   
float sinTime = sin(time);   
   
  int i = round( abs( inPos[1]) ); 
  
float4 tempPos;

 tempPos[0] = (inPos[0] + vectors[i][0]) ;
 tempPos[1] = (0        + vectors[i][1]) ;  
 tempPos[2] = (inPos[2] + vectors[i][2]) ;   
 tempPos[3] =  inPos[3];    
////////////////////////////////
   // Grab X and Y eye-space vectors in world-space ...
   float3 dirX = matView[0];
   float3 dirY = matView[1];

   // ... which are used for billboarding
   float3 posMd = tempPos + 1.0 * (tempPos.x * dirX + tempPos.y * dirY);

   Out.Pos = mul(matViewProj, float4(posMd, 1));
   
   Out.Tex = 0.5 * inPos.xy + 0.5; 
////////////////////////
/*
   Out.Pos =   mul( matViewProj, tempPos ); 
     
   Out.Tex = (inTex - f1MyFloat )/ f1MyFloat_2;
*/
   return Out;
}



]]></RmShaderCode>
