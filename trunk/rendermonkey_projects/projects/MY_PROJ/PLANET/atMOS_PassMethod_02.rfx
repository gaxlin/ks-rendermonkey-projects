    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="32"><RmShaderCode TYPE="TEXT"><![CDATA[
//float myFloat;
float blurFactor;
sampler2D TextureImage;

static const int g_cKernelSize = 5;  //  old  13

//float2 texelKernel[13];

static const float BlurWeights[g_cKernelSize] = 
{
    0.2,    // 0.002216,
    //0.008764,
    //0.026995,
    0.2,
    //0.120985,
    //0.176033,
    
   
    
    //0.176033,
    //0.120985,
    0.2,
    //0.026995,
    //0.008764,
    0.2,   // 0.002216,
    
    
     0.3, // 0,0    // 0.4
};

float2 PixelKernel[g_cKernelSize] =
{
    { -1,  1 }, 
   // { -5,  0 },
   // { -4,  0 },
    
    {  1,  -1 }, 
    //{ -2,  0 },
    //{ -1,  0 },

    
    //{  1,  0 }, 
   // {  2,  0 },
    {  1,  1 },
    
   // {  4,  0 }, 
   // {  5,  0 },
    { -1,  -1 },
        
    {  0,  0 },    
}; 

//-----------------------------------------------------------------------------
// Pixel Shader: HorizontalBlur
// Desc: Blurs the image horizontally
//-----------------------------------------------------------------------------
float4 ps_main( float2 texCoord  : TEXCOORD0 ) : COLOR
{
    float4 Color = 0;

    for (int i = 0; i < g_cKernelSize; i++)
    {                                           // texelKernel
        Color += tex2D( TextureImage, texCoord + PixelKernel[i].xy     * blurFactor * 0.002 )    *   BlurWeights[i] * 0.94;
    }
    
    return Color;

   //   OLD  -> return tex2D( TextureImage, texCoord );   
} // end PS

