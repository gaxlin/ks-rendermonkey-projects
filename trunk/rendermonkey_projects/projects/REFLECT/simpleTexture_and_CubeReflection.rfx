     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="main" COMPILER_FLAGS="32"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 matWorld;
float4x4 view_proj_matrix: register(c0);
float4 view_position: register(c4);
struct VS_OUTPUT {
   float4 Pos:     POSITION;
   float3 normal:  TEXCOORD0;
   float3 viewVec: TEXCOORD1;
   float2 tc :TEXCOORD2;
};

VS_OUTPUT main(float4 Pos: POSITION, float3 normal: NORMAL, float2 txtrCoor :TEXCOORD){
   VS_OUTPUT Out;

float4x4 mtr = mul (view_proj_matrix , matWorld );
   Out.Pos = mul(mtr, Pos);
   Out.normal = normal;
   Out.viewVec =    view_position - Pos  ;
   Out.tc = txtrCoor;
   return Out;
}







]]></RmShaderCode>
