     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="main" COMPILER_FLAGS="32"><RmShaderCode TYPE="TEXT"><![CDATA[
float briRefl;
sampler2D Texture1;
float f1MyFloat;
float reflVectCorr_Z;
float reflVectCorr_Y;
float reflVectCorr_X;
 
float bright;
//float rainbowiness: register(c0);
sampler Environment: register(s1);
//sampler Rainbow: register(s0);

float4 main(
         float3 normal  : TEXCOORD0, 
         float3 viewVec : TEXCOORD1,
         float2 txtrCoor :TEXCOORD2   ) : COLOR 
{
   normal = normalize(normal);

   float v = dot(normalize(viewVec), normal) * 2.0;
   
   // Find the reflection
   float3 reflVec = reflect(-viewVec, normal);
    reflVec[0] *= -reflVectCorr_X;
    reflVec[1] *= reflVectCorr_Y;    
    reflVec[2] *= reflVectCorr_Z;    
    
   
   float3 refl = (briRefl * texCUBE(Environment, reflVec)) + (tex2D( Texture1 , txtrCoor) ) ;

   float3 color = lerp(refl, 0, 0);
          color *= bright;
    
  return float4(color, 1 - v );
}






]]></RmShaderCode>
