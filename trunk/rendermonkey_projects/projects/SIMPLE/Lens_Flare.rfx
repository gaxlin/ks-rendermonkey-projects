    <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="vs_main"><RmShaderCode TYPE="TEXT"><![CDATA[
float time;
float2 p1_scalePos;
float p1_scaleXY;
float p1_scale;
float scaleXY;

float commonScale;
float2 commonScaleVec2;
float2 commonLightSourcPositVec2;

struct VS_OUTPUT
{
   float4 pos       : POSITION0;
   float2 texCoord  : TEXCOORD0;
};

VS_OUTPUT vs_main( float4 inPos: POSITION )
{
   VS_OUTPUT ou = (VS_OUTPUT) 0;

 float4 modPos = inPos;
  
 float2 makeScale = commonScaleVec2 * commonScale;
        makeScale[0] *=  p1_scale * p1_scaleXY ;
        makeScale[1] *=  p1_scale / p1_scaleXY;
  
   modPos.x = modPos.x * makeScale[0];
   modPos.y = modPos.y * makeScale[1];
       
float2 lsp;
  lsp[0] = commonLightSourcPositVec2[0] * p1_scalePos[0];       
  lsp[1] = commonLightSourcPositVec2[1] * p1_scalePos[1];       
       
   modPos.x =   modPos.x + lsp[0] ;
   modPos.y =   modPos.y - lsp[1] ;
   
   
   ou.pos = float4( modPos.xy,     0.0f   ,  1  ); //   ou.pos = float4( inPos.xy,      0.0f   ,   imageScale  );



   // get into range [0,1]
ou.texCoord = 
(float2
( (modPos.x - lsp[0]) * 1/makeScale[0],  (modPos.y +  lsp[1]) * 1/makeScale[1] ) + 1.0f   ) 
    / 2.0f;  
 

   return ou;
}
]]></RmShaderCode>
     <RmShaderConstant NAME="commonLightSourcPositVec2" API="" REGISTER="5"/>
     <RmShaderConstant NAME="commonScale" API="" REGISTER="3"/>
     <RmShaderConstant NAME="commonScaleVec2" API="" REGISTER="4"/>
     <RmShaderConstant NAME="p1_scale" API="" REGISTER="2"/>
     <RmShaderConstant NAME="p1_scalePos" API="" REGISTER="0"/>
     <RmShaderConstant NAME="p1_scaleXY" API="" REGISTER="1"/>
    </RmHLSLShader>
    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="ps_main"><RmShaderCode TYPE="TEXT"><![CDATA[
float p1_colorMultFactor;
float4 p1_correctColor;
//float4 color;
//float alpha;
float f1MyFloat;


float4 commonColorCorrect;
float4 firstGlowColor;
 
sampler2D Texture0;

float4 ps_main( float2 texCoord  : TEXCOORD0 ) : COLOR
{
  float4 resultColor = tex2D( Texture0, texCoord );
  resultColor *= p1_correctColor * commonColorCorrect * p1_colorMultFactor;;

   return resultColor;
}

]]></RmShaderCode>
