float f1MyFloat_2;
sampler2D Texture0;

float4 ps_main( float2 texCoord  : TEXCOORD0 ) : COLOR
{
  float2 md  = texCoord *  f1MyFloat_2; 
 // float2 md2 = texCoord * time * 


float4 retColor1 = tex2D( Texture0, texCoord );
float4 retColor2 = tex2D( Texture1, (texCoord + time * 0.7) * 0.1 );



   return (retColor1 + retColor2 * 0.4) * colorTone;
}

]]></RmShaderCode>
