     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="vs_main"><RmShaderCode TYPE="TEXT"><![CDATA[
float f1MyFloat_2;
float f1MyFloat_1;
float scale;
float f1MyFloat;
float heigth;
float width;
float left;
float top;

float fViewportHeight;
float fViewportWidth;


struct VS_OUTPUT
