   float2 texCoord  : TEXCOORD0;
};

VS_OUTPUT vs_main( float4 inPos: POSITION )
{
   VS_OUTPUT o = (VS_OUTPUT) 0;

float asp = fViewportWidth/fViewportHeight;

float4 modCoord = inPos;
  modCoord.xy = sign( modCoord.xy);
  
  
  
float4 outCoord = float4( modCoord.xy, 0.0f, 1.0f);
 
  outCoord.x = outCoord.x *scale;
  outCoord.y = outCoord.y *scale; 
    
   outCoord.x = outCoord.x + f1MyFloat;
   
float2 modTxtCoord = (float2(outCoord.x * (1/scale), -outCoord.y* (1/scale)) + 1.0f)/2.0f ; 

  modTxtCoord.x = modTxtCoord.x * f1MyFloat_1;
  modTxtCoord.y = modTxtCoord.y * f1MyFloat_1; 
  modTxtCoord.x = modTxtCoord.x + f1MyFloat_2;
   
  
   o.texCoord = modTxtCoord;
   o.pos = outCoord;
   return o;
}
