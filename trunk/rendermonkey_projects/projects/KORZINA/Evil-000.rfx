float sqrAtt: register(c2);

float4 main(float3 normal: TEXCOORD0, float3 lightVec: TEXCOORD1) : COLOR {
   // Simple world-space lighting
   normal = normalize(normal);
   float3 lVec = normalize(lightVec);
   float atten = 1 / (constAtt + sqrAtt * dot(lightVec, lightVec));

   float diffuse = dot(lVec, normal);

   return lightColor * diffuse * atten;
