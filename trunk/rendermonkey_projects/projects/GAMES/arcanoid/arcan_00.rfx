float4x4 matViewProjection;

struct VS_OUTPUT 
{
   float4 Pos : POSITION;
   float2 Tex : TEXCOORD0;
};

VS_OUTPUT vs_main( float4 inPos: POSITION, float2 inTex: TEXCOORD )
{
   VS_OUTPUT Out;

   Out.Pos = mul( matViewProjection, inPos );
   
   Out.Tex = inTex;

   return Out;
}



]]></RmShaderCode>
