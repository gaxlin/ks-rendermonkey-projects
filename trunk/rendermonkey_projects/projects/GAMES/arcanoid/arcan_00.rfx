      <RmShaderConstant NAME="Ka" API="" REGISTER="4"/>
      <RmShaderConstant NAME="diffuse" API="" REGISTER="1"/>
      <RmShaderConstant NAME="n_specular" API="" REGISTER="3"/>
      <RmShaderConstant NAME="specular" API="" REGISTER="2"/>
