     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_1_1" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[
struct VS_OUTPUT {
   float4 Pos: POSITION;
   float2 texCoord: TEXCOORD;
};

VS_OUTPUT main(float4 Pos: POSITION){
   VS_OUTPUT Out;

   // Clean up inaccuracies
   Pos.xy = sign(Pos.xy);

   Out.Pos = Pos;
   // Image-space
   Out.texCoord.x = 0.5 * (1 + Pos.x);
   Out.texCoord.y = 0.5 * (1 - Pos.y);

   return Out;
}

]]></RmShaderCode></RmHLSLShader>
     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[
sampler Image: register(s0);
// PI * 2
const float PI2 = 6.2831853;

float4 main(float2 texCoord: TEXCOORD) : COLOR {
   float4 rgba = tex2D(Image, texCoord);

   float r = rgba.r;
   float g = rgba.g;
   float b = rgba.b;

   float rg = r - g;
   float rb = r - b;
   // Hue
   float h = acos((rg + rb) / (2 * sqrt(rg * rg + rb * (g - b)))) / PI2;
   if (b > g) h = 1 - h;

   // Intensity
   float i = (r + g + b) / 3;
   // Saturation
   float s = 1 - min(min(r, g), b) / i;

   return float4(h,s,i, 0);
}





]]></RmShaderCode>
