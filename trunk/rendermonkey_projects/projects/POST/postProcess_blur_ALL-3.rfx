static const int g_cKernelSize = 13;  //  old  13

//float2 texelKernel[13];

static const float BlurWeights[g_cKernelSize] = 
{
    0.002216,
    0.008764,
    0.026995,
    0.064759,
    0.120985,
    0.176033,
    0.199471, // 0,0
    0.176033,
    0.120985,
    0.064759,
    0.026995,
