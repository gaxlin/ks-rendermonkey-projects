    {  0,  0 },
    
    {  1,  0 },  // III
    {  2,  0 },
    {  3,  0 },
    
    {  4,  0 },  // IV
    {  5,  0 },
    {  6,  0 },
}; 




//-----------------------------------------------------------------------------
// Pixel Shader: HorizontalBlur
// Desc: Blurs the image horizontally
//-----------------------------------------------------------------------------
float4 ps_main( float2 texCoord  : TEXCOORD0 ) : COLOR
{
    float4 Color = 0;

    for (int i = 0; i < g_cKernelSize; i++)
    {                                           // texelKernel
        Color += tex2D( TextureImage, texCoord + PixelKernel[i].xy     * blurFactor * 0.002 )    *   BlurWeights[i] * 1;
    }
    
    return Color;

   //   OLD  -> return tex2D( TextureImage, texCoord );   
} // end PS

]]></RmShaderCode>
