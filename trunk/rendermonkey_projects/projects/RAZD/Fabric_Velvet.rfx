    <RmColorVariable NAME="Kd" API="" ARTIST_EDITABLE="TRUE" VARIABLE_SEMANTIC="" VALUE_0="0.834503" VALUE_1="0.871622" VALUE_2="0.860217" VALUE_3="1.000000"/>
    <RmColorVariable NAME="sheen" API="" ARTIST_EDITABLE="TRUE" VARIABLE_SEMANTIC="" VALUE_0="0.000000" VALUE_1="0.811765" VALUE_2="0.136659" VALUE_3="1.000000"/>
    <RmColorVariable NAME="base_color" API="" ARTIST_EDITABLE="TRUE" VARIABLE_SEMANTIC="" VALUE_0="0.125452" VALUE_1="1.000000" VALUE_2="0.928592" VALUE_3="1.000000"/>
    <RmColorVariable NAME="shiny" API="" ARTIST_EDITABLE="TRUE" VARIABLE_SEMANTIC="" VALUE_0="0.574324" VALUE_1="0.096053" VALUE_2="0.150329" VALUE_3="1.000000"/>
    <RmColorVariable NAME="Ks" API="" ARTIST_EDITABLE="TRUE" VARIABLE_SEMANTIC="" VALUE_0="0.690476" VALUE_1="0.159991" VALUE_2="0.000000" VALUE_3="1.000000"/>
    <RmPass NAME="Single Pass" API="D3D" PASS_INDEX="0" ENABLED="TRUE">
     <RmModelReference NAME="mesh" API=""/>
     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_1_1" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 view_matrix;
float4x4 view_proj_matrix;
struct VS_OUTPUT
{
   float4 Pos     : POSITION;
   float3 TCoord0 : TEXCOORD0;
   float3 TCoord1 : TEXCOORD1;
};


VS_OUTPUT main (float4 vPosition: POSITION, float3 vNormal: NORMAL)
{
   VS_OUTPUT Out = (VS_OUTPUT) 0; 

   Out.Pos = mul (view_proj_matrix, vPosition);

   Out.TCoord0 = mul (view_matrix, vPosition);
   Out.TCoord1 = mul (view_matrix, vNormal);

   return Out;
}



]]></RmShaderCode>
      <RmShaderConstant NAME="view_matrix" API="" REGISTER="0"/>
      <RmShaderConstant NAME="view_proj_matrix" API="" REGISTER="4"/>
     </RmHLSLShader>
     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="hlsl_velvet"><RmShaderCode TYPE="TEXT"><![CDATA[
float4 eye_pos;
float4 Kd;
float4 light_color;
float4 light_pos;
float4 Ka;
float4 global_ambient;
float backscatter;
float edginess;
float roughness;
float4 shiny;
float4 sheen;
float4 base_color;
float4 ambient(void)
{
   return global_ambient;
}

float4 diffuse(float3 Neye, float3 Peye)
{
   // Compute normalized vector from vertex to light in eye space  (Leye)
   float3 Leye = (light_pos - Peye) / length(light_pos - Peye);

   float NdotL = dot(Neye, Leye);

   // N.L
   return float4(NdotL, NdotL, NdotL, NdotL);
}


float4 hlsl_velvet (float3 Peye : TEXCOORD0, float3 Neye : TEXCOORD1) : COLOR
{
   float3 Nf = normalize(Neye);           // Normalized normal vector
   float3 Veye = -(Peye / length(Peye));  // Normalized eye vector

   // For every light do the following:

      // Hemisphere
      float3 Leye = (light_pos - Peye) / length(light_pos - Peye);             // Leye for a given light

      // Retroreflective lobe
      float cosine = clamp(dot(Leye, Veye), 0.0f, 1.0f);
      shiny += pow (cosine, 1.0f / roughness ) * backscatter * light_color * sheen;

      // Horizon scattering
      cosine = clamp (dot(Nf, Veye), 0.0f, 1.0f);
      float sine = sqrt (1.0 - (cosine * cosine));
      shiny += pow (sine, edginess) * dot(Leye, Nf) * light_color * sheen;

   // Uncomment one of these to see different components of illumination
//   return Ka*ambient();
//   return Kd*diffuse(Neye, Peye);
//   return shiny;

   // Add in diffuse color and return
   return (Ka*ambient() + Kd*diffuse(Neye, Peye)) * base_color + shiny;
}



]]></RmShaderCode>
      <RmShaderConstant NAME="Ka" API="" REGISTER="3"/>
      <RmShaderConstant NAME="Kd" API="" REGISTER="0"/>
      <RmShaderConstant NAME="backscatter" API="" REGISTER="5"/>
      <RmShaderConstant NAME="base_color" API="" REGISTER="10"/>
      <RmShaderConstant NAME="edginess" API="" REGISTER="6"/>
      <RmShaderConstant NAME="global_ambient" API="" REGISTER="4"/>
      <RmShaderConstant NAME="light_color" API="" REGISTER="1"/>
      <RmShaderConstant NAME="light_pos" API="" REGISTER="2"/>
      <RmShaderConstant NAME="roughness" API="" REGISTER="7"/>
      <RmShaderConstant NAME="sheen" API="" REGISTER="9"/>
      <RmShaderConstant NAME="shiny" API="" REGISTER="8"/>
     </RmHLSLShader>
