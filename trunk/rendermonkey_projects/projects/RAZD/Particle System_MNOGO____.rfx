     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_1_1" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 view_proj_matrix: register(c0);
float4x4 view_matrix: register(c4);
float time_0_X: register(c8);
struct VS_OUTPUT {
   float4 Pos: POSITION;
   float2 texCoord: TEXCOORD0;
   float color: TEXCOORD1;
};

VS_OUTPUT main(float4 Pos: POSITION){
   VS_OUTPUT Out;

   float3 pos;
   // Billboard the quads.
   // The view matrix gives us our right and up vectors.
   pos = 15 * Pos.z * (Pos.x * view_matrix[0] + Pos.y * view_matrix[1]);

   // Move the quads around along some odd path
   float t = time_0_X + 4 * Pos.z;
   pos.x += 200 * cos(1.24 * t*0.2) * sin(3.12 * t*0.2);
   pos.y += 200 * sin(2.97 * t*0.2) * cos(0.81 * t*0.2);
   pos.z += 200 * cos(t*0.2) * sin(t * 1.231*0.2);

   Out.Pos = mul(view_proj_matrix, float4(pos, 1));
   Out.texCoord = Pos.xy;
   Out.color = Pos.z;

   return Out;
}]]></RmShaderCode>
