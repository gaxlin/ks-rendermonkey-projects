     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[
float particleShape: register(c0);
const float4 diffuseColor = float4(1, 0.7, 0, 0);

float4 main(float3 normal: TEXCOORD0, float3 lightVec: TEXCOORD1, float3 viewVec: TEXCOORD2) : COLOR {
   float3 lVec = normalize(lightVec);
   normal = normalize(normal);

   // Basic lighting
   float diffuse = dot(lVec, normal);
   float specular = pow(saturate(dot(reflect(-normalize(viewVec), normal), lVec)), 8);
   // Fine-tuned attenuation for this scene
   float atten = saturate((0.2 + particleShape) / (0.15 + 0.0001 * dot(lightVec, lightVec)));

   return atten * (diffuse * diffuseColor + specular);
}


]]></RmShaderCode>
      <RmShaderConstant NAME="particleShape" API="" REGISTER="0"/>
     </RmHLSLShader>
     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_1_1" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 view_proj_matrix: register(c0);
float4 particleSystemPosition: register(c4);
float4 view_position: register(c5);
float particleSystemHeight: register(c6);
struct VS_OUTPUT {
   float4 Pos:      POSITION;
   float3 normal:   TEXCOORD0;
   float3 lightVec: TEXCOORD1;
   float3 viewVec:  TEXCOORD2;
};

VS_OUTPUT main(float4 Pos: POSITION, float3 normal: NORMAL){
   VS_OUTPUT Out;

   Out.Pos = mul(view_proj_matrix, Pos);
   // World-space lighting
   Out.normal = normal;
   Out.lightVec = particleSystemPosition - Pos;
   // Move z up a little to center the light a bit up
   // in the particle system
   Out.lightVec.z += 0.3 * particleSystemHeight;
   Out.viewVec = view_position - Pos;

   return Out;
}





]]></RmShaderCode>
