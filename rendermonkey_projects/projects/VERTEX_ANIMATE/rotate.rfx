     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_1_1" ENTRY_POINT="vs_main"><RmShaderCode TYPE="TEXT"><![CDATA[
float time;
float4x4 matViewProjection;

struct VS_OUTPUT 
{
   float4 Pos : POSITION;
   float2 Tex : TEXCOORD0;
};

float4x4 RotateX (float ang)
{
float4x4 mRes = 0;
 mRes[0][0]  = 1;   mRes[0][1]   = 0;    mRes[0][2]  = 0;   mRes[0][3]   = 0;
 
 mRes[1][0]  = 1;   mRes[1][1]   = 0;    mRes[1][2]  = 0;   mRes[1][3]   = 0;
 
 mRes[2][0]  = 1;   mRes[2][1]   = 0;    mRes[2][2]  = 0;   mRes[2][3]   = 0; 
 
 mRes[3][0]  = 1;   mRes[3][1]   = 0;    mRes[3][2]  = 0;   mRes[3][3]   = 0;

return mRes;

};



VS_OUTPUT vs_main( float4 inPos: POSITION, float2 inTex: TEXCOORD )
{
   VS_OUTPUT Out;

   float sn;
   float cs;
sincos(time, sn, cs);

float4x4 world = RotateX(time);
//float4x4 mRotX;


float4 modPos = inPos;

float4x4 wvp = mul(matViewProjection, world);


   Out.Pos = mul( wvp, modPos );
   
   Out.Tex = inTex;

   return Out;
}



]]></RmShaderCode>
