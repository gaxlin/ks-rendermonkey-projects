    <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="main" COMPILER_FLAGS="32"><RmShaderCode TYPE="TEXT"><![CDATA[
float3 lightPosit;
float4x4 g_matView;
float4x4 g_matWorldViewProj;
float planetRadius;
 
 
 // float4 lightDir;

struct VS_OUTPUT
{
   float4 Pos    : POSITION;
   float3 Norm   : TEXCOORD0;
   float3 View   : TEXCOORD1;
   float3 Light  : TEXCOORD2;
   float2 Tex    : TEXCOORD4;
};
VS_OUTPUT main( 
   float4 inPos  : POSITION,
   float3 inNorm : NORMAL,
   float2 inTex  : TEXCOORD0 )
{
   VS_OUTPUT Out = (VS_OUTPUT) 0; 

float4 modPos;
  modPos[0] = inPos[0] * planetRadius;
  modPos[1] = inPos[1] * planetRadius;
  modPos[2] = inPos[2] * planetRadius;
  modPos[3] = inPos[3];


   // Output transformed position:
   Out.Pos = mul( g_matWorldViewProj, modPos ); 
   
   // Output light vector:
   
  Out.Light = mul(g_matView,  normalize(lightPosit)    ); 

   // Compute position in view space:
   float3 Pview = mul( g_matView, inPos ); 

   // Transform the input normal to view space:
   Out.Norm = normalize( mul( g_matView, inNorm ) );   
 
   // Compute the view direction in view space:
   Out.View = - normalize( Pview );

/*
   // Output transformed position:
   Out.Pos = mul( g_matWorldViewProj, modPos ); 
   
   // Output light vector:
   Out.Light = normalize(lightDir);

   // Compute position in view space:
   float3 Pview = mul( g_matView, modPos ); 

   // Transform the input normal to view space:
   Out.Norm = normalize( mul( g_matView, inNorm ) );   
 
   // Compute the view direction in view space:
   Out.View = - normalize( Pview );
*/



   // Propagate texture coordinate for the object:
   Out.Tex = inTex;

   return Out;
}




]]></RmShaderCode>
