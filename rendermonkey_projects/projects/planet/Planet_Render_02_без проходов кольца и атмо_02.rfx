    <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="main" COMPILER_FLAGS="32"><RmShaderCode TYPE="TEXT"><![CDATA[

float4 planetPos;

float3 lightPosit;

float4x4 g_matView;
float4x4 g_matViewProjection;
float4x4 g_matWorld;

float planetRadius;
 
 
 // float4 lightDir;

struct VS_OUTPUT
{
   float4 Pos    : POSITION;
   float3 Norm   : TEXCOORD0;
   float3 View   : TEXCOORD1;
   float3 Light  : TEXCOORD2;
   float2 Tex    : TEXCOORD4;
};
VS_OUTPUT main( 
   float4 inPos  : POSITION,
   float3 inNorm : NORMAL,
   float2 inTex  : TEXCOORD0 )
{
   VS_OUTPUT Out = (VS_OUTPUT) 0; 

float4 modPos = inPos;
 
   // Output transformed position:
   
  float4x4 mWVP = mul(   g_matViewProjection ,  g_matWorld  ); 
       Out.Pos = mul(  mWVP ,  modPos ); 
 
 
  float4x4 mWV = mul(   g_matView , g_matWorld   );
  
  
 float3 newlightPosit = lightPosit;
 /*
  newlightPosit[0] += g_matWorld[0][0];
  newlightPosit[1] += g_matWorld[0][1];
  newlightPosit[2] += g_matWorld[0][2];    
  */

    newlightPosit = mul( mWV ,  newlightPosit   );
   Out.Light =  normalize( newlightPosit ) ;

   // Compute position in view space:
   float3 Pview = mul( mWV, inPos ); 

   // Transform the input normal to view space:
   Out.Norm = normalize( mul( mWV, inNorm ) );   
 
   // Compute the view direction in view space:
   Out.View = - normalize( Pview );
 

   // Propagate texture coordinate for the object:
   Out.Tex = inTex;

   return Out;
}




]]></RmShaderCode>
