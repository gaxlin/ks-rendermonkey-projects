      <RmShaderConstant NAME="Ka" API="" REGISTER="6"/>
      <RmShaderConstant NAME="diffuse" API="" REGISTER="2"/>
      <RmShaderConstant NAME="n_specular" API="" REGISTER="5"/>
      <RmShaderConstant NAME="specular" API="" REGISTER="4"/>
