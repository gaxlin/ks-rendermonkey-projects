 mRes[2][0]  = 0;   mRes[2][1]   = 0;    mRes[2][2]  = 1;   mRes[2][3]   = 0; 
 
 mRes[3][0]  = 0;   mRes[3][1]   = 0;    mRes[3][2]  = 0;   mRes[3][3]   = 1;

