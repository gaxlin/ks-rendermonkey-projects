sincos(ang, sn, cs);

 mRes[0][0]  = cs;   mRes[0][1]   = 0;    mRes[0][2]  = -sn;   mRes[0][3]   = 0;
 
 mRes[1][0]  = 0;   mRes[1][1]   = 1;    mRes[1][2]  = 0;   mRes[1][3]   = 0;
 
