     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[
sampler AmbOcclusion;
sampler DiffuseEnvironment;
float4 main(float3 N: TEXCOORD0, float3 Neye : TEXCOORD1, float2 tcBase: TEXCOORD2) : COLOR
{
   // Sample the filtered environment map
   float4 ambient = texCUBE(DiffuseEnvironment, N);


   // Sample the ambient occlusion map
   float  ambientOcclusion = tex2D(AmbOcclusion, tcBase);

   return ambient * ambientOcclusion;
}






]]></RmShaderCode>
      <RmSampler NAME="AmbOcclusion" API="" REGISTER="0"/>
      <RmSampler NAME="DiffuseEnvironment" API="" REGISTER="1"/>
     </RmHLSLShader>
     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_1_1" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[
float4x4 view_proj_matrix;
float4x4 inv_view_matrix;
float time_0_2PI;
float time_scale;
struct VS_OUTPUT
{
   float4 Pos    : POSITION;
   float3 N      : TEXCOORD0;
   float3 Neye   : TEXCOORD1;
   float2 tcBase : TEXCOORD2;
};

VS_OUTPUT main(float4 Pos: POSITION, float3 vNormal: NORMAL, float2 tcBase:TEXCOORD0)
{
   VS_OUTPUT Out = (VS_OUTPUT) 0;

   // Tiny little model .. give it some size and center it better
   Out.Pos    = mul(view_proj_matrix, Pos);

   float3 Nworld = mul(vNormal, inv_view_matrix); // Intrinsic argument ordering effectively multiplies by transpose of inverse view matrix

   // Lock the environment around the camera but rotate it slowly around y
   // 
   // Effectively multiply by the following matrix:
   //
   // [ cos(time)      0   -sin(time)]
   // [    0           1       0     ]
   // [ sin(time)      0    cos(time)]
   //

   // Rotate the normal back the other way so lighting is correct
   float3 rotNorm = Nworld;
   rotNorm .x = dot(Nworld.xyz, float3(cos(-time_scale*time_0_2PI), 0, -sin(-time_scale*time_0_2PI)));
   rotNorm .z = dot(Nworld.xyz, float3(sin(-time_scale*time_0_2PI), 0,  cos(-time_scale*time_0_2PI)));

   Out.N      = rotNorm ;

   Out.tcBase = tcBase;

   return Out;
}







]]></RmShaderCode>
