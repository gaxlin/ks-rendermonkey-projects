     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="ps_main"><RmShaderCode TYPE="TEXT"><![CDATA[
float f1MyFloat;

int resolution;

sampler2D Texture_sourceImage;
sampler2D Texture_smalBlurSourceImage;

sampler2D Texture_r_mask;
sampler2D Texture_g_mask;
sampler2D Texture_b_mask;

sampler2D Texture_rgbMASK;


float4 ps_main( float2 texCoord  : TEXCOORD0 ) : COLOR
{


float4 sourceColor = tex2D( Texture_sourceImage, texCoord * f1MyFloat );



float4 Rcolor = sourceColor[0] * tex2D( Texture_r_mask, texCoord * resolution );
float4 Gcolor = sourceColor[1] * tex2D( Texture_g_mask, texCoord * resolution );
float4 Bcolor = sourceColor[2] * tex2D( Texture_b_mask, texCoord * resolution );



float4 retColor;
retColor[0] = Rcolor[0];
retColor[1] = Gcolor[1];
retColor[2] = Bcolor[2];
retColor[3] = 1.0;
  
   return retColor ;   
}

]]></RmShaderCode>
      <RmSampler NAME="Texture_b_mask" API="" REGISTER="3"/>
      <RmSampler NAME="Texture_g_mask" API="" REGISTER="2"/>
      <RmSampler NAME="Texture_r_mask" API="" REGISTER="1"/>
      <RmSampler NAME="Texture_sourceImage" API="" REGISTER="0"/>
      <RmShaderConstant NAME="f1MyFloat" API="" REGISTER="0"/>
      <RmShaderConstant NAME="resolution" API="" REGISTER="1"/>
     </RmHLSLShader>
     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="vs_main"><RmShaderCode TYPE="TEXT"><![CDATA[
struct VS_OUTPUT
{
   float4 pos       : POSITION0;
   float2 texCoord  : TEXCOORD0;
};

VS_OUTPUT vs_main( float4 inPos: POSITION )
{
   VS_OUTPUT o = (VS_OUTPUT) 0;

   inPos.xy = sign( inPos.xy);
   o.pos = float4( inPos.xy, 0.0f, 1.0f);

   // get into range [0,1]
   o.texCoord = (float2(o.pos.x, -o.pos.y) + 1.0f)/2.0f;
   return o;
}
]]></RmShaderCode></RmHLSLShader>
