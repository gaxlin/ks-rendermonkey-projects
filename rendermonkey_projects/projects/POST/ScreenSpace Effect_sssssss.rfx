      <RmShaderConstant NAME="distortionScale" API="" REGISTER="4"/>
      <RmShaderConstant NAME="frameLimit" API="" REGISTER="8"/>
      <RmShaderConstant NAME="frameSharpness" API="" REGISTER="1"/>
      <RmShaderConstant NAME="interference" API="" REGISTER="7"/>
