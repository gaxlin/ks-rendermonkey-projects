     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="vs_main" COMPILER_FLAGS="32"><RmShaderCode TYPE="TEXT"><![CDATA[
struct VS_OUTPUT
{
   float4 pos       : POSITION0;
   float2 texCoord  : TEXCOORD0;
};

VS_OUTPUT vs_main( float4 inPos: POSITION )
{
   VS_OUTPUT o = (VS_OUTPUT) 0;

   inPos.xy = sign( inPos.xy);
   o.pos = float4( inPos.xy, 0.0f, 1.0f);

   // get into range [0,1]
   o.texCoord = (float2(o.pos.x, -o.pos.y) + 1.0f)/2.0f;
   return o;
}
]]></RmShaderCode></RmHLSLShader>
     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="ps_main" COMPILER_FLAGS="32"><RmShaderCode TYPE="TEXT"><![CDATA[
float blue;
float green;
float red;
float contrast;
float brigh;

sampler2D Texture0;

float4 ps_main( float2 texCoord  : TEXCOORD0 ) : COLOR
{
  float4 retColor = 0;
    retColor =  tex2D( Texture0, texCoord );
   
 ///  contrast  ///   
 float rg = ((contrast/2 - 1) + 0.5 ) * (abs( 0.5 - retColor[0] ) / 0.5) ;  
 float gg = ((contrast/2 - 1) + 0.5 ) * (abs( 0.5 - retColor[1] ) / 0.5) ;  
 float bg = ((contrast/2 - 1) + 0.5 ) * (abs( 0.5 - retColor[2] ) / 0.5) ;

    if (retColor[0] > 0.5)     retColor[0] = retColor[0]+ rg ; //  else { retColor[0] =- rg };
        if (retColor[0] < 0.5) retColor[0] = retColor[0]- rg ;
        
    if (retColor[1] > 0.5)     retColor[1] = retColor[1]+ gg ; // else { retColor[1] =- rg };
        if (retColor[1] < 0.5) retColor[1] = retColor[1]- gg ;
        
    if (retColor[2] > 0.5)     retColor[2] = retColor[2]+ bg ; // else { retColor[2] =- rg };
        if (retColor[2] < 0.5) retColor[2] = retColor[2]- bg ;
 
 /// brigh  ///
   retColor *= brigh ;  
 
 /// red green blue ///
   retColor[0] *= red; 
   retColor[1] *= green;  
   retColor[2] *= blue;  
  
  return retColor ; 
}

]]></RmShaderCode>
