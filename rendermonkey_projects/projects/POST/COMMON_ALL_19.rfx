struct VS_OUTPUT
{
   float4 pos       : POSITION0;
   float2 texCoord  : TEXCOORD0;
};

VS_OUTPUT vs_main( float4 inPos: POSITION )
{
   VS_OUTPUT o = (VS_OUTPUT) 0;

  // inPos.xy = sign( inPos.xy);
  
  float vdrojScaleImage = 1/drojScaleImage;
  
  
  float modTime = g_time * drojSpeed; 
  
  float vAnimX = sin (modTime * 0.845) + sin(modTime * 1.215) + sin (modTime * 0.925) ;
  float vAnimY = sin (modTime * 0.756) + sin(modTime * 1.203) + sin (modTime * 0.758) ;  
  
  float offsetX =+ vAnimX*drojScale* 0.01; 
  float offsetY =+ vAnimY*drojScale* 0.01;
  
   o.pos = float4(  (inPos.x-offsetX) / vdrojScaleImage , (inPos.y-offsetY) / vdrojScaleImage   ,  0.0f, 1.0f  );
 
   o.texCoord = (  float2( (o.pos.x+offsetX)* vdrojScaleImage , -(o.pos.y+offsetY)* vdrojScaleImage  ) + 1.0f  )
                   / 2.0f;
  // o.texCoord = (float2(o.pos.x, -o.pos.y) + 1.0f)/2.0f;                
                   
                   
   return o;
}
