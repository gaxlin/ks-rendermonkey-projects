    0.2,
    //0.026995,
    //0.008764,
    0.2,   // 0.002216,
    
    
     0.3, // 0,0    // 0.4
};

float2 PixelKernel[g_cKernelSize] =
{
    { -1,  1 }, 
   // { -5,  0 },
   // { -4,  0 },
    
    {  1,  -1 }, 
    //{ -2,  0 },
    //{ -1,  0 },

    
    //{  1,  0 }, 
   // {  2,  0 },
    {  1,  1 },
    
   // {  4,  0 }, 
   // {  5,  0 },
    { -1,  -1 },
        
    {  0,  0 },    
}; 

//-----------------------------------------------------------------------------
// Pixel Shader: HorizontalBlur
// Desc: Blurs the image horizontally
//-----------------------------------------------------------------------------
float4 ps_main( float2 texCoord  : TEXCOORD0 ) : COLOR
{
    float4 Color = 0;

    for (int i = 0; i < g_cKernelSize; i++)
    {                                           // texelKernel
        Color += tex2D( TextureImage, texCoord + PixelKernel[i].xy     * blur_2x2_factor * 0.002 )    *   BlurWeights[i] * 0.94;
    }
    
    return Color;

   //   OLD  -> return tex2D( TextureImage, texCoord );   
} // end PS

