sampler2D TextureImage;

static const int g_cKernelSize = 5;  //  old  13

//float2 texelKernel[13];

static const float BlurWeights[g_cKernelSize] = 
{
    0.2,    // 0.002216,
    //0.008764,
    //0.026995,
    0.2,
    //0.120985,
    //0.176033,
    
   
    
    //0.176033,
