    <RmColorVariable NAME="color" API="" ARTIST_EDITABLE="TRUE" VARIABLE_SEMANTIC="" VALUE_0="0.696206" VALUE_1="1.000000" VALUE_2="0.677758" VALUE_3="1.000000"/>
    <Rm3DTextureVariable NAME="Noise" API="" ARTIST_EDITABLE="FALSE" VARIABLE_SEMANTIC="" FILE_NAME="..\..\Media\Textures\NoiseVolume.dds" ORIGIN="0"/>
