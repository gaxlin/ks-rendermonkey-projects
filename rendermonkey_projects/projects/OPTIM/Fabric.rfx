     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="hlsl_velvet"><RmShaderCode TYPE="TEXT"><![CDATA[
float4 eye_pos;
float4 Kd;
float4 light_color;
float4 light_pos;
float4 Ka;
float4 global_ambient;
float backscatter;
float edginess;
float roughness;
float4 shiny;
float4 sheen;
float4 base_color;
float4 ambient(void)
{
   return global_ambient;
}

float4 diffuse(float3 Neye, float3 Peye)
{
   // Compute normalized vector from vertex to light in eye space  (Leye)
   float3 Leye = (light_pos - Peye) / length(light_pos - Peye);

   float NdotL = dot(Neye, Leye);

   // N.L
   return float4(NdotL, NdotL, NdotL, NdotL);
}


float4 hlsl_velvet (float3 Peye : TEXCOORD0, float3 Neye : TEXCOORD1) : COLOR
{
   float3 Nf = normalize(Neye);           // Normalized normal vector
   float3 Veye = -(Peye / length(Peye));  // Normalized eye vector

   // For every light do the following:

      // Hemisphere
      float3 Leye = (light_pos - Peye) / length(light_pos - Peye);             // Leye for a given light

      // Retroreflective lobe
      float cosine = clamp(dot(Leye, Veye), 0.0f, 1.0f);
      shiny += pow (cosine, 1.0f / roughness ) * backscatter * light_color * sheen;

      // Horizon scattering
      cosine = clamp (dot(Nf, Veye), 0.0f, 1.0f);
      float sine = sqrt (1.0 - (cosine * cosine));
      shiny += pow (sine, edginess) * dot(Leye, Nf) * light_color * sheen;

   // Uncomment one of these to see different components of illumination
//   return Ka*ambient();
//   return Kd*diffuse(Neye, Peye);
//   return shiny;

   // Add in diffuse color and return
   return (Ka*ambient() + Kd*diffuse(Neye, Peye)) * base_color + shiny;
}



]]></RmShaderCode>
