      <RmShaderConstant NAME="multColorFactor" API="" REGISTER="6"/>
      <RmShaderConstant NAME="noiseSpeed" API="" REGISTER="3"/>
      <RmShaderConstant NAME="fadeBias" API="" REGISTER="4"/>
      <RmShaderConstant NAME="time_0_X" API="" REGISTER="0"/>
