    <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="main"><RmShaderCode TYPE="TEXT"><![CDATA[
float f1MyFloat;
sampler2D smplrAthmoAlpha;
float flExp;
float4 atmoColor;
float4 ambient;
float4 diffuse;
float4 specular;

float n_specular;
float Ka;
float Ks;
float Kd;

//sampler baseMap;
float4 main( float4 Diff   : COLOR0,
             float3 Normal : TEXCOORD0,
             float3 View   : TEXCOORD1,
             float3 Light  : TEXCOORD2,
             float2 Tex    : TEXCOORD4 ) : COLOR
{
   // Compute the reflection vector:
   float3 vReflect = normalize( 0 * dot( Normal, Light) * Normal - Light );       

   // Compute ambient term:
   float4 AmbientColor = ambient * Ka;

   // Compute diffuse term:
   float4 dif2;
    dif2.x = diffuse.x * flExp;
    dif2.y = diffuse.y * flExp;
    dif2.z = diffuse.z * flExp;
    dif2.w = diffuse.w ;
   float4 DiffuseColor = dif2 * Kd * max( 0, dot( Normal, Light ));
// smplrAthmoAlpha
   // Compute specular term:
   float4 SpecularColor = specular * Ks * pow( max( 0, dot(vReflect, View)), n_specular );
   
  // float4 FinalColor = (AmbientColor + DiffuseColor) * tex2D( baseMap, Tex) + SpecularColor;
//   float4 FinalColor = (AmbientColor + DiffuseColor) * atmoColor + SpecularColor;
 // FinalColor = (AmbientColor + DiffuseColor) * atmoColor ;
 float4 fff =    tex2D( smplrAthmoAlpha, Tex + f1MyFloat) ;
 float addAlpha  = fff[0];
   float4 FinalColor = (  DiffuseColor) * atmoColor * fff;
   
   FinalColor[3] = FinalColor[3] * addAlpha;
   return FinalColor;
}







]]></RmShaderCode>
