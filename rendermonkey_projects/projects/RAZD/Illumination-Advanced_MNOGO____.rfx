    <RmCamera NAME="Camera" API="" CAMERA_POSITION_X="-27.697250" CAMERA_POSITION_Y="-99.303337" CAMERA_POSITION_Z="43.864468" LOOK_AT_VECTOR_X="0.000000" LOOK_AT_VECTOR_Y="0.000000" LOOK_AT_VECTOR_Z="0.000000" UP_VECTOR_X="0.077726" UP_VECTOR_Y="0.384616" UP_VECTOR_Z="0.919799" FOV="45.000000" NEAR_CLIP_PLANE="1.000000" FAR_CLIP_PLANE="1000.000000"/>
    <RmPass NAME="Single Pass" API="D3D" PASS_INDEX="0" ENABLED="TRUE">
