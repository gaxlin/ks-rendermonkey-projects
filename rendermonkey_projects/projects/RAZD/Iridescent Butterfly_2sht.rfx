ps.2.0
def    c0, 0, .5, 1, 2
def    c1, 4, 0, 0, 0

dcl_2d s0 // Base and Opacity
dcl_2d s1 // Normal and Gloss Map
dcl_2d s2 // Rainbow Gradient
dcl    t0.xy // Base coordinates
dcl    t1.xyz // Tangent Space View Vector
dcl    t2.xyz // Tangent Space Light
dcl    t3.xyz // Tangent Space Halfangle

// Diffuse calculations.
texld   r1, t0, s1              // Normal and Gloss map
mad     r1.xyz, r1, c0.w, -c0.z // _bx2 normal      
dp3_sat r4.y, r1, t2            // sat(N.L)
dp3_sat r4.w, r1, -t2           // sat(-N.L)
texld   r0, t0, s0              // Base and opacity

mul     r4.w, r4.w, r0.a        // opacity*-N.L
mad     r5.w, r4.w, c1.x, r4.y  // opacity*-N.L + N.L
add     r5.rgb, r5.w, c3        // += ambient
mul     r0.rgb, r0, r5          // (N.L + ambient)*Base
sub_sat r0.a, c0.z, r0.a        // 1-opacity

// Iridescence
dp3     r6.xy, r1, t1          // N.V
dp3_sat r6.y, r1, t3           // N.H
mad     r6.y, r6.y, c4.x, c4.y // Scale and bias N.H
mul     r6.z, r6.y, r1.w       // *= gloss map
mad     r6.x, r6.x, c4.z, c4.w // Scale and bias iridescence color change speed
texld   r2, r6, s2             // Iridescence map
mul     r0.rgb, r0, r0.a       // Premultiply alpha blend to avoid clamping.
mad     r0.rgb, r6.z, r2, r0   // N.H*Gloss*Iridescence + Diffuse

mov oC0, r0

]]></RmShaderCode>
