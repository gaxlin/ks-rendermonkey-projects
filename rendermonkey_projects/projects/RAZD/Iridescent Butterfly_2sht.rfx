     <RmShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE"><RmShaderCode TYPE="TEXT"><![CDATA[
vs.1.1

def c102, 0, .5, 1, 2
def c103, 0, 0, 0, 1

dcl_position     v0
dcl_normal       v1
dcl_texcoord     v2
dcl_tangent      v3
dcl_binormal     v4
 
// Output transformed vertex position: 
m4x4 oPos, v0, c1

// Propagate input texture coordinates:
mov  oT0.xy, v2.xy

// Compute the light vector:
mov r1, c0
m4x4 r0, r1, c5      // Light position in object space
sub  r0, r0, v0      // Light vector = light position - vertex position

// Normalize the light vector:
dp3 r1.w, r0, r0     // Length^2
rsq r1.w, r1.w       // 1/Length
mul r0.xyz, r0, r1.w // normalized Light vector    R0 = LIGHT VECTOR

// Output the light vector in tangent space:
dp3 oT2.x, r0, v3    // Tangent
dp3 oT2.y, r0, v4    // Binormal
dp3 oT2.z, r0, v1    // Normal

// Compute the view vector:
mov r1, c103
m4x4 r2, r1, c5      // View position in object space
sub  r2, r2, v0      // View vector = view position - vertex position

// Normalize the view vector:
dp3 r1.w, r2, r2     // Length^2
rsq r1.w, r1.w       // 1/Length
mul r2.xyz, r2, r1.w // normalized View vector    R2 = VIEW VECTOR

// Output the view vector in tangent space:
dp3 oT1.x, r2, v3    // Tangent
dp3 oT1.y, r2, v4    // Normal
dp3 oT1.z, r2, v1    // Normal

// Invert normal when on the back side of the polygon 
mov r4, v1              // Normal
dp3 r3.w, r2, r4        // V * N
slt r3.w, r3.w, c102.z
mad r3.w, r3.w, c102.w, -c102.z
mul r4.xyz, r4.xyz, -r3.w

// Compute the half angle vector:
add r5.xyz, r2, r0       // H = V + L

// Normalize the half angle vector:
dp3 r1.w, r5, r5     // Length^2
rsq r1.w, r1.w       // 1/Length
mul r5.xyz, r5, r1.w // normalized View vector    R2 = VIEW VECTOR

// Output the half angle vector in tangent space:
dp3 oT3.x, r5, v3    // Tangent
dp3 oT3.y, r5, v4    // Binormal
dp3 oT3.z, r5, v1    // Normal

]]></RmShaderCode>
