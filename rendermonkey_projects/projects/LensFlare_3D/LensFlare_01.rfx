     <RmHLSLShader NAME="Pixel Shader" API="D3D" PIXEL_SHADER="TRUE" TARGET="ps_2_0" ENTRY_POINT="main" COMPILER_FLAGS="32"><RmShaderCode TYPE="TEXT"><![CDATA[
 
float txtrCoordScale;

float4 color: register(c0);
 
 
float coronaExp: register(c3);
float strength: register(c4);
 
sampler Corona: register(s0);

float4 main(float2 texCoord: TEXCOORD0) : COLOR {


 float2 modTxtrCoord = texCoord;
 
 modTxtrCoord[0] *= txtrCoordScale;
 modTxtrCoord[0] =  modTxtrCoord[0] -  txtrCoordScale/2; //modTxtrCoord[0]; 
 
 modTxtrCoord[1] *= txtrCoordScale;
 modTxtrCoord[1] =  modTxtrCoord[1] -  txtrCoordScale/2;  
 
  modTxtrCoord[0] +=     0.5;
  modTxtrCoord[1] +=     0.5; 
 
   float4 corona = tex2D(Corona, modTxtrCoord);
 
   // Glow depending on cosine (dotprod) with "corona directon"
   float glow = saturate(  5.5);

  float4 retColot = strength * pow(glow, coronaExp) * corona * color;
 
   return retColot;
}







]]></RmShaderCode>
      <RmSampler NAME="Corona" API="" REGISTER="0"/>
      <RmShaderConstant NAME="color" API="" REGISTER="0"/>
      <RmShaderConstant NAME="strength" API="" REGISTER="4"/>
      <RmShaderConstant NAME="txtrCoordScale" API="" REGISTER="1"/>
     </RmHLSLShader>
     <RmHLSLShader NAME="Vertex Shader" API="D3D" PIXEL_SHADER="FALSE" TARGET="vs_2_0" ENTRY_POINT="main" COMPILER_FLAGS="32"><RmShaderCode TYPE="TEXT"><![CDATA[
float txtrCoordScale;
 
float height;
float width;
float size;
 
float4x4 matView: register(c4);
float4x4 matViewProj: register(c0);
 
float4 posit: register(c8);
 
struct VS_OUTPUT {
   float4 Pos: POSITION;
   float2 texCoord: TEXCOORD0;
};

VS_OUTPUT main(float4 inPos: POSITION, float2 txCrd :TEXCOORD0)
{
   VS_OUTPUT Out;

   // Grab X and Y eye-space vectors in world-space ...
   float3 dirX = matView[0];
   float3 dirY = matView[1];

   // ... which are used for billboarding
   float3 modPos = posit + size *  (inPos.x * dirX * width + inPos.y * dirY * height);
 
  
   Out.Pos = mul(matViewProj, float4(modPos, 1));
   
   float2 mdTxtrCoord = txCrd;
  
   Out.texCoord = mdTxtrCoord;   

   return Out;
}




]]></RmShaderCode>
