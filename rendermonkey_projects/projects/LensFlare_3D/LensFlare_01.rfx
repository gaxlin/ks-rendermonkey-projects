VS_OUTPUT vs_main( float4 inPos: POSITION )
{
   VS_OUTPUT ou = (VS_OUTPUT) 0;

  float4 modPos = inPos;
  
 float2 makeScale;
 
   makeScale[0] =   (commonScale * commonScaleXY) * (scale * scaleXY)  ;
   makeScale[1] =   (commonScale / commonScaleXY) * (scale / scaleXY)  ;  
  
