struct VS_OUTPUT 
{
   float4 Pos : POSITION;
   float2 Tex : TEXCOORD0;
};

VS_OUTPUT vs_main( float4 inPos: POSITION, float2 inTex: TEXCOORD )
{
   VS_OUTPUT Out;

 float4 mdPos = inPos; 
 mdPos[0] *= scaleTerrain; 
 mdPos[1] *= scaleTerrain; 
 mdPos[2] *= scaleTerrain; 

   Out.Pos = mul( matViewProjection, mdPos );
   
   Out.Tex = inTex;

   return Out;
}



